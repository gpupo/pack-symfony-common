{
    "name": "gpupo/pack-symfony-common",
    "description": "Pacote de componentes de uso comum em projetos que rodam Symfony Framework",
    "homepage": "https://opensource.gpupo.com/pack-symfony-common/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["common", "development"],
    "license": "MIT",
    "require": {
      "php": ">=7.4 || ^8.0",

      "ext-bcmath": "*",
      "ext-iconv": "*",
      "ext-intl": "*",
      "ext-mbstring": "*",
      "ext-pdo": "*",
      "ext-ctype": "*",

      "symfony-bundles/slugify": "^1.0",
      "symfony/asset": "^5.1",
      "symfony/console": "^5.1",
      "symfony/dotenv": "^5.1",
      "symfony/dependency-injection": "^5.1",
      "symfony/flex": "^1.2",
      "symfony/form": "^5.1",
      "symfony/framework-bundle": "^5.1",
      "symfony/http-client": "^5.1",
      "symfony/lock": "^5.1",
      "symfony/monolog-bundle": "^3.5|dev-master",
      "symfony/orm-pack": "*",
      "symfony/process": "^5.1",
      "symfony/messenger": "^5.1",
      "symfony/security-bundle": "^5.1",
      "symfony/security-csrf": "^5.1",
      "symfony/serializer-pack": "*",
      "symfony/templating": "^5.1",
      "symfony/validator": "^5.1",
      "symfony/web-link": "^5.1",
      "symfony/web-server-bundle": "*",
      "symfony/webpack-encore-bundle": "^1.7",
      "symfony/workflow": "^5.1",
      "symfony/yaml": "^5.1",
      "symfony/translation": "^5.1",
      "symfony/twig-pack": "*",
      "symfony/apache-pack": "^1.0",
      "symfony/intl": "^5.1",
      "symfony/mailer": "^5.1",
      "symfony/notifier": "^5.1",
      "symfony/string": "^5.1",
      "symfony/expression-language": "^5.1",
      "jms/serializer-bundle": "^3.5",
      "sensio/framework-extra-bundle": "^5.5",
      "php-translation/symfony-bundle": "^0.10",
      "gpupo/common-sdk": "^5.1 || ^6.0",
      "doctrine/doctrine-migrations-bundle": "^3.0",
      "jackalope/jackalope-doctrine-dbal": "^1.3",
      "antishov/doctrine-extensions-bundle": "^1.4",
      "nelmio/cors-bundle": "^2.0",
      "ocramius/proxy-manager": "^2.5",
      "willdurand/negotiation": "^2.3",
      "vich/uploader-bundle": "^1.12",
      "php-amqplib/php-amqplib": "^2.11",
      "nyholm/psr7": "*",
      "symfony/psr-http-message-bridge": "*"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\PackSymfonyCommon\\": "src",
            "Gpupo\\PackSymfonyCommon\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "gpupo/pack-symfony-dev": "*"
    },
    "scripts": {
        "test": "APP_ENV=test vendor/bin/simple-phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
